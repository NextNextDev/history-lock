{
  "manifest_version": 2,
  "name": "History Lock",
  "description": "Блокирует историю браузера",
  "version": "1.0",
  "icons": { "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png" },
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "History Lock",
    "default_popup": "main.html"
  }
}